        <TextBox Grid.Column="1" HorizontalAlignment="Left" Margin="875,684,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBlock Grid.Column="1" HorizontalAlignment="Left" Margin="760,431,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="18"><Run Language="pl-pl" Text="Imie"/><LineBreak/><Run Language="pl-pl"/></TextBlock>
        <TextBlock Grid.Column="1" HorizontalAlignment="Left" Margin="760,468,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="83"><Run Language="pl-pl" Text="Drugie Imie"/></TextBlock>
        <TextBlock Grid.Column="1" HorizontalAlignment="Left" Margin="760,505,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="83"><Run Language="pl-pl" Text="Nazwisko"/><LineBreak/><Run Language="pl-pl"/></TextBlock>
